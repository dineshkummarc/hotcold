  "main": "",
  "authors": [
    "palerdot"
  ],
  "license": "",
  "keywords": [
    "touch-typing"
  ],
  "homepage": "http://hotcoldtyping.com",
  "moduleType": [
    "globals"
  ],
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "2.2.4",
    "bootstrap": "^3.3.6",
    "components-font-awesome": "^4.6.3",
    "underscore": "^1.8.3"
